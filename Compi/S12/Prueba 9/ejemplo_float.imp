var float y;

fun int sumarec(int x)
  print(x);
  if (x < 1) then
    return (0)
  else
    return (x + sumarec(x-1))
  endif;
  print(1000)
endfun

fun float factorial(float x)
  print(x);
  if (x < 1) then
    return (1.0)
  else
    return (x * factorial(x-1))
  endif;
  print(1000)
endfun

fun void main()
  y = factorial(5.1);
  print(y);
  return ()
endfun
